{
  "manifest_version": 2,
  "name": "Splunkstr",
  "version": "0.5",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.2.1.min.js"]
  }
],
  "permissions": [
    "contextMenus", "tabs", "storage"
  ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
  "icons": ["icon.svg.png"],
  "default_icon": "icon.svg.png",
  "default_popup": "popup.html"
}
}
